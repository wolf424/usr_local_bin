    POKEMON_REPTINCEL)
	pic="$PIC_POKEMON_REPTINCEL";;
    POKEMON_SALAMECHE)
    	pic="$PIC_POKEMON_SALAMECHE";;
    POKEMON_TORTANK)
    	pic="$PIC_POKEMON_TORTANK";;
    SEALION)
    	pic="$PIC_SEALION";;
    SS)
    	pic="$PIC_SS";;
    SWASTIKA)
    	pic="$PIC_SWASTIKA";;
    SWASTIKA2)
    	pic="$PIC_SWASTIKA2";;
    SWASTIKA3)
    	pic="$PIC_SWASTIKA3";;
    ZKW1)
    	pic="$PIC_ZKW1";;
    ZKW2)
	pic="$PIC_ZKW2";;
    \?) # Invalid option
	pic="$PIC_HEARTRAINBOW";;
esac

curl -X POST "http://$WLED_DEVICE/json/state" -d "$pic" -H "Content-Type: application/json" >/dev/null 2>/dev/null
