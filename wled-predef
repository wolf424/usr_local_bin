Help()
{
   # Display Help
   echo "display preselected picture on wled $WLED_DEVICE"
   echo
   echo "Syntax: $0 [-h|-n name]"
   echo "options:"
   echo "-h     Print this Help."
   echo "-n     name of the preselected picture"
   echo "       88 88LGBT ALIEN1 ALX3 FRANCELIBRE HEART12 HEART14 HEART34 HEARTEMPTY HEARTFULLBLUE HEARTFULLGREEN HEARTRAINBOW NUCLEAR PIKACHU PIKACHU2 POKEMON POKEMON_BULBIZARRE POKEMON_CARAPUCE POKEMON_DRACAUFEU POKEMON_FLORIZARRE POKEMON_HERBIZARRE POKEMON_PIKACHU POKEMON_REPTINCEL POKEMON_SALAMECHE POKEMON_TORTANK SEALION SS SWASTIKA SWASTIKA2 SWASTIKA3 ZKW1 ZKW2"
   echo
}

name="heartrainbow"
# Get the options
while getopts ":hn:" option; do
    case $option in
	h) # display Help
            Help
            exit;;
	n) # name of predefined picture
            name=$OPTARG;;
	\?) # Invalid option
	    Help
            exit;;
    esac
done

if [ "$#" -eq 0 ]; then
    Help
    exit
fi

case $name in
    ss)
	pic="$PIC_SS";;
